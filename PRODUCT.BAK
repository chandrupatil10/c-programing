#include<stdio.h>
int main()
{
int arr1[2][2],arr2[2][2],product[2][2]={0},i,j,k;
printf("enter element of first matrix (2*2):");
for(i = 0 ; i<2 ; i++){
for(j=0;j<2;j++){
scanf("%d",&arr2[i][j]);
}
}
printf("enter element of second matrix(2*2):");
for(i =0;i<2;i++){
for(j=0;j<2;j++){
scanf("%d",&arr2[i][j]);
}
}
for(i=0;i<2;i++){
for(j=0;j<2;j++){
for(k=0;k<2;k++)
product[i][j] +=arr1[i][j]* arr2[k][j];
}
printf("\n");
}
return 0;
}